<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


    <filter>
        <filter-name>staticResourcesCacheFilter</filter-name>
        <filter-class>no.nav.sbl.internarbeidsflatedecorator.CacheFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>staticResourcesCacheFilter</filter-name>
        <url-pattern>navnSlikAtIngentingTrefferForLokalTesting</url-pattern>
    </filter-mapping>


    <servlet>
        <servlet-name>restservlet</servlet-name>
        <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
        <init-param>
            <param-name>jersey.config.server.provider.packages</param-name>
            <param-value>no.nav.sbl.mock</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
        <async-supported>true</async-supported>
    </servlet>
    <servlet-mapping>
        <servlet-name>restservlet</servlet-name>
        <url-pattern>/api/*</url-pattern>
    </servlet-mapping>

</web-app>
